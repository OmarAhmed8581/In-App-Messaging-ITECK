<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/frameLayout"
    android:layout_width="match_parent" android:layout_height="wrap_content"
    tools:context=".FuelFragment">

    <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/constraintLayouttt"
        android:layout_width="match_parent" android:layout_height="300dp"
        app:layout_constraintTop_toTopOf="parent">

        <com.github.mikephil.charting.charts.HorizontalBarChart android:id="@+id/horibar"
            android:layout_width="match_parent" android:layout_height="70dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="parent" tools:ignore="MissingConstraints" />

        <TextView android:id="@+id/textView3" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:fontFamily="@font/helvetica"
            android:text="973" android:textColor="#000" android:textSize="22dp"
            app:layout_constraintBottom_toTopOf="@+id/horibar"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView android:id="@+id/textView4" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="4dp"
            android:layout_marginBottom="2dp" android:fontFamily="@font/helvetica"
            android:text="Litres" android:textColor="#A19A9A"
            app:layout_constraintBottom_toBottomOf="@+id/textView3"
            app:layout_constraintStart_toEndOf="@+id/textView3" />

        <com.github.mikephil.charting.charts.HorizontalBarChart android:id="@+id/horibar2"
            android:layout_width="match_parent" android:layout_height="70dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/horibar" tools:ignore="MissingConstraints" />

        <TextView android:id="@+id/textView5" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:fontFamily="@font/helvetica"
            android:text="806" android:textColor="#000" android:textSize="22dp"
            app:layout_constraintBottom_toTopOf="@+id/horibar2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView android:id="@+id/textView6" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="4dp"
            android:layout_marginBottom="2dp" android:fontFamily="@font/helvetica"
            android:text="Litres" android:textColor="#A19A9A"
            app:layout_constraintBottom_toTopOf="@+id/horibar2"
            app:layout_constraintStart_toEndOf="@+id/textView5" />

        <ImageView android:id="@+id/imageView2" android:layout_width="24dp"
            android:layout_height="24dp" android:layout_marginStart="8dp"
            android:layout_marginTop="10dp" android:padding="4dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/fuelwhite" app:tint="#00AFCA" />

        <TextView android:id="@+id/textView9" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="4dp"
            android:text="Fuel" android:textColor="#00AFCA" android:textSize="20dp"
            android:textStyle="bold" app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2" />

        <TextView android:id="@+id/textView10" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="4dp"
            android:text="You have burned 97.4 litres in past 28 days."
            android:textAlignment="viewStart" android:textColor="#000" android:textSize="14dp"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <LinearLayout android:id="@+id/textView11" android:layout_width="match_parent"
            android:layout_height="1dp" android:layout_margin="4dp"
            android:background="@drawable/line_drawable" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10" />

        <TextView android:id="@+id/textView12" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="8dp"
            android:fontFamily="@font/helvetica" android:text="This Month" android:textColor="#fff"
            android:textSize="12dp" app:layout_constraintBottom_toBottomOf="@+id/horibar"
            app:layout_constraintStart_toStartOf="@id/horibar"
            app:layout_constraintTop_toTopOf="@+id/horibar" />

        <TextView android:id="@+id/textView13" android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_marginStart="8dp"
            android:fontFamily="@font/helvetica" android:text="Last Month" android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/horibar2"
            app:layout_constraintStart_toStartOf="@id/horibar2"
            app:layout_constraintTop_toTopOf="@+id/horibar2" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>


